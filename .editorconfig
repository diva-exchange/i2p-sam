# EditorConfig helps developers define and maintain consistent
# coding styles between different editors and IDEs
# editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
tab_width = 2
trim_trailing_whitespace = true
insert_final_newline = false
max_line_length = 120

[*.md]
trim_trailing_whitespace = false

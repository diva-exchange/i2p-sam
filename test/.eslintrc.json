{
  "env": {
    "node": true
  },
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended"
  ],
  "rules": {
    "max-classes-per-file": 0,
    "max-line-length": 0,
    "no-empty": 0,
    "only-arrow-functions": 0,
    "no-console": 0,
    "object-literal-sort-keys": 0,
    "interface-name": 0,
    "ban-types": 0,
    "no-shadowed-variable": 0,
    "forin": 0,
    "member-ordering": 0,
    "deprecation": 0,
    "no-reference": 0,
    "trailing-comma": 0,
    "no-unused-vars": 0
  }
}